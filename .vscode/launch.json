{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "PureHttpMcpClient",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/client/bin/Debug/net9.0/PureHttpMcpClient.dll",
            "cwd": "${workspaceFolder}/client",
            "stopAtEntry": false,
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "preLaunchTask": "build-client"
        }
    ]
}